{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "forOf": true,
    "jsx": true,
    "es6": true
  },
  "parserOptions" : {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "forOf": true,
      "es6": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/no-named-as-default": [0],
    "react/forbid-prop-types": [0],
    "react/no-unused-prop-types": [2, {"skipShapeProps": true}],
    "no-underscore-dangle": ["error", { "allow": ["_error"] }]
  }
}